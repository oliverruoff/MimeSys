{
  "name": "3D Home Digital Twin",
  "version": "1.0.3",
  "slug": "home-digital-twin",
  "description": "A 3D digital twin of your home with interactive floor plans, lighting control, and real-time visualization",
  "arch": ["aarch64", "amd64"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "host_network": true,
  "map": ["config:rw", "share:rw"],
  "options": {
    "data_dir": "/data"
  },
  "schema": {
    "data_dir": "str?"
  },
  "image": "oruoff/{arch}-addon-home-digital-twin",
  "url": "https://github.com/oliverruoff/MimeSys",
  "ingress": true,
  "ingress_port": 8000,
  "ingress_entry": "/",
  "panel_icon": "mdi:home-3d",
  "panel_title": "Home Digital Twin",
  "webui": "http://[HOST]:[PORT:8000]",
  "homeassistant_api": true,
  "hassio_api": true,
  "hassio_role": "default"
}
